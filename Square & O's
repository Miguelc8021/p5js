<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>

<head>
    <title>p5js Template</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.6.0/p5.js"></script>
    <script>
      function setup() {
  createCanvas(400, 400);

}

function mouseMoved() {
  background(mouseX/2,mouseY,260) // the background will change colors as the mouse is moved around the screen.
  rect(mouseX/2,mouseY/2,100,100) // the square
  ellipse(mouseX/2,mouseY/2,50,100) // top left circle
  ellipse(mouseX,mouseY,50,100) // bottom right circle
  
}

function keyPressed() {
  fill(mouseX,mouseY,30)
  // as any key is pressed the color of the shapes will change
}

function mouseClicked() {
  stroke(100,10,133) // color of the line
  strokeWeight(5) // thickness of the line
  line(400,0,mouseX,mouseY)
  line(0,400,mouseX,mouseY)
  line(400,0,mouseX/2,mouseY/2)
  line(0,400,mouseX/2,mouseY/2)
}
    </script>
</head>

<body>
    <div id="code" class="container-fluid">
        <script src="https://gist-it.appspot.com/github/Miguelc8021/p5js/blob/master/04-interaction-project.html?footer=0"></script>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
    <script>
        $(window).bind("load", function() {
            $('#code').before($('#defaultCanvas0').remove());
        });
    </script>
</body>


</html> 
